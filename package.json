{
  "name": "fogata-automatic",
  "version": "0.1.0",
  "description": "Script to automate payments in Fogata",
  "author": "Julian Gonzalez",
  "scripts": {
    "build": "tsc",
    "copy:abis": "copyfiles -u 2 fogata/build/fogata-abi.json src && copyfiles -u 2 fogata/build/pools-abi.json src",
    "lint": "yarn lint:prettier && yarn lint:eslint && yarn lint:tsc",
    "lint:prettier": "prettier . --check",
    "lint:eslint": "eslint . --ext .js,.ts",
    "lint:tsc": "tsc --noEmit --incremental false",
    "start:dev": "ts-node src/index | ts-node src/transport",
    "start": "wrap () { node dist/index.js \"$@\" | node dist/transport.js; }; wrap",
    "start:once": "node dist/index.js --no-daemon"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "dotenv": "^16.0.1",
    "koilib": "^5.3.1",
    "pump": "^3.0.0",
    "split2": "^4.1.0",
    "through2": "^4.0.2"
  },
  "devDependencies": {
    "@tsconfig/node12": "^1.0.11",
    "@types/node": "^18.7.13",
    "@types/pump": "^1.1.1",
    "@types/split2": "^3.2.1",
    "@types/through2": "^2.0.38",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "audit-ci": "^6.3.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tsdoc": "^0.2.16",
    "prettier": "^2.7.1",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.7.4"
  }
}
